{
    "name": "jsthis",
    "displayName": "JsThis",
    "description": "Better IntelliSense for JavaScript \"this\" keyword",
    "version": "0.0.1",
    "publisher": "obrejla",
    "author": {
        "name": "Ond≈ôej Brejla",
        "email": "ondrej@brejla.cz"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/obrejla/vscode-jsthis.git"
    },
    "engines": {
        "vscode": "^0.10.7"
    },
    "categories": [
        "Languages"
    ],
    "activationEvents": [
        "onLanguage:javascript"
    ],
    "main": "./src/main",
    "contributes": {
        "configuration": {
            "title": "JsThis configuration",
            "properties": {
                "jsthis.thisVarNames": {
                    "type": "array",
                    "default": ["self"],
                    "description": "Variable names to be also considered as 'this' variable"
                }
            }
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "estraverse": "4.1.1",
        "estraverse-fb": "1.3.1",
        "esprima": "2.7.2"
    },
    "devDependencies": {
        "vscode": "^0.11.1"
    },
    "license": "MIT"
}