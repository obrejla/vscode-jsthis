{
    "name": "jsthis",
    "displayName": "JsThis",
    "description": "Better IntelliSense for JavaScript \"this\" keyword",
    "version": "0.0.1",
    "publisher": "obrejla",
    "engines": {
        "vscode": "^0.10.6"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:javascript"
    ],
    "main": "./src/main",
    "contributes": {
        "configuration": {
            "title": "JsThis configuration",
            "properties": {
                "jsthis.thisVarNames": {
                    "type": "array",
                    "default": ["self"],
                    "description": "Variable names to be also considered as 'this' variable"
                }
            }
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "dependencies": {
        "estraverse": "4.1.1",
        "estraverse-fb": "1.3.1",
        "esprima": "2.7.2"
    },
    "devDependencies": {
        "vscode": "^0.11.0"
    },
    "license": "MIT"
}